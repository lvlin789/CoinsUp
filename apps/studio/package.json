{
  "name": "prisma-studio",
  "version": "0.0.0",
  "scripts": {
    "dev": "prisma studio --schema ../../packages/database/prisma/schema.prisma --port 3005",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
